.one-tab
  position: fixed
  bottom: 0
  width: 100%
  display: flex
  height: 40px
  line-height: 40px
  color: #DDDDDD
  text-align: center
  background: #fff
  .tab-item
    flex: 1
  .nav-link, .link
    display: block
    color: #949292
    text-decoration: none
    &.active
      color: black
    .iconfont
      font-size: 24px